@article{velizInfluenceGlobalRankings2022,
  title = {Influence of Global Rankings on Strategic Planning from the Perspective of Decision‐makers: {{A}} Case Study of a {{Chilean}} Research University},
  shorttitle = {Influence of Global Rankings on Strategic Planning from the Perspective of Decision‐makers},
  author = {Véliz, Daniela and Marshall, Pìo},
  date = {2022},
  journaltitle = {Higher Education Quarterly},
  shortjournal = {Higher Education Quarterly},
  volume = {76},
  number = {3},
  pages = {638--652},
  issn = {0951-5224, 1468-2273},
  doi = {10.1111/hequ.12333},
  url = {https://onlinelibrary.wiley.com/doi/10.1111/hequ.12333},
  urldate = {2025-11-25},
  abstract = {Abstract             This paper complements and extends the literature analysing the relationship of global rankings on universities' strategies, missions, structures, and functions. Specifically, it examines the extent to which global rankings have influenced development strategies of one Chilean university through administrator's perception and changes in the strategic planning. Interview and documentary data were collected at two points in time (2014 and 2017), mainly through semi‐structured interviews with 18 university administrators and college deans. Findings suggest a relationship between the university's ranking position and its perception of academic excellence. However, rankings have effects that are not only perceptual. High ranking positions increase global visibility, promote international partnerships and collaborations that benefit academic staff. At the same time, the university finds itself increasingly bound by the current global standards on academic search, appointment, promotion, and evaluation. Findings suggest that universities are rarely passive recipients of the influence of rankings, they seek opportunities to leverage them to their strategic advantage, more when the ranking benefits their visibility.           ,              Abstracta             Este artículo complementa y amplía la literatura que analiza la relación de los rankings globales sobre las estrategias, misiones, estructuras y funciones de las universidades. Específicamente, examina hasta qué punto los rankings globales han influido en las estrategias de desarrollo de una universidad chilena a través de la percepción de los administradores y los cambios producidos en la planificación estratégica. Los datos se basan en entrevistas y documentos recopilaron en dos momentos (2014 y 2017). Principalmente a través de entrevistas semiestructuradas con 18 administradores y decanos universitarios. Los hallazgos sugieren una relación entre la posición en el ranking de la universidad y su percepción de excelencia académica. Los hallazgos sugieren que las universidades rara vez son receptoras pasivas de la influencia de las clasificaciones, buscan oportunidades para aprovecharlas en su ventaja estratégica, más cuando la clasificación beneficia su visibilidad.},
  langid = {english}
}
